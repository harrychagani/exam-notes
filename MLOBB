import numpy as np
from sklearn.ensemble import RandomForestRegressor

X = np.array([
    [-1.32046087,  0.98628976, -1.27222272,  1.07805524],
    [-0.69468444,  0.79206326,  1.32045413,  0.19293365],
    [ 1.83856654, -0.88814492, -0.24951873, -0.0511031 ],
    [ 0.83807192, -0.52768148,  0.04303932,  0.42056878],
    [-0.29675542, -1.17721229, -1.64252144, -1.09807964],
    [-0.21059996, -1.79470189, -1.08073234,  0.65145195],
    [ 0.92901938, -1.18758506,  0.27145336,  1.15609793],
    [ 0.59539852,  0.12016381, -1.84045452, -1.09631787],
    [ 0.43935618,  0.94494881, -0.88066443,  1.10818735],
    [ 0.50045488, -0.86306024,  0.06572327,  0.44139931],
    [ 0.48864073,  2.88515655,  0.28527648,  0.09246106],
    [ 0.92137182,  0.12475117, -0.68526138,  1.89683153]
])

y = np.array([
    39.46165741, 104.56500935, -15.55175778, 30.79692071, -313.9912708,
    -175.89975802, 56.30169015, -175.25876066, 141.08341219, -9.7567785,
    259.98584025, 172.5703366
])

model = RandomForestRegressor(
    n_estimators=58,
    random_state=159,
    oob_score=True,
    bootstrap=True
)
model.fit(X, y)

# Step 3: Output the OOB score
print("Out-of-Bag R^2 Score:", model.oob_score_)
